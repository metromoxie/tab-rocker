{
  "name": "Tab Return",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Returns to the previous tab when you click on a tab",
  "permissions": [
    "http://*/*",
    "https://*/*",
	"tabs"
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "content_scripts": [{
    "matches": [ "http://*/*", "https://*/*" ],
    "js": [ "content.js" ]
  }]
}
